  "version": 2,
  "functions": {
    "api/index.php": { "runtime": "vercel-php@0.4.1" }
  },
  "routes": [
    {
      "src": "/(.*)\\.(.*)$",
      "dest": "public/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
